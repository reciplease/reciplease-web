{
  "expo": {
    "privacy": "public",
    "name": "reciplease-client",
    "slug": "reciplease",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "../assets/src/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "../assets/src/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "web",
      "android",
      "ios"
    ],
    "web": {
      "favicon": "../assets/src/images/favicon.png"
    },
    "android": {
      "package": "org.reciplease.android"
    },
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}
